<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="kz.uco.tsadv.web.injuredorgan.InjuredOrganEdit"
        datasource="injuredOrganDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web.injuredorgan">
    <dsContext>
        <datasource id="injuredOrganDs"
                    class="kz.uco.tsadv.entity.tb.InjuredOrgan"
                    view="injuredOrgan-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="injuredOrganDs">
            <column width="250px">
                <field property="injuredOrgan"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
