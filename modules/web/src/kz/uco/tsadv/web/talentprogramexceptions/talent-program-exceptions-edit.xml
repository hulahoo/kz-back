<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="kz.uco.tsadv.web.talentprogramexceptions.TalentProgramExceptionsEdit"
        datasource="talentProgramExceptionsDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web.talentprogramexceptions">
    <dsContext>
        <datasource id="talentProgramExceptionsDs"
                    class="kz.uco.tsadv.entity.TalentProgramExceptions"
                    view="talentProgramExceptions-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="talentProgramExceptionsDs">
            <column width="250px">
                <field property="talentProgram"/>
                <field property="personGroup"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
