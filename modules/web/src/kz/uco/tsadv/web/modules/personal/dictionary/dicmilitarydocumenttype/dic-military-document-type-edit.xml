<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://DicMilitaryDocumentTypeEdit"
        class="kz.uco.tsadv.web.modules.personal.dictionary.dicmilitarydocumenttype.DicMilitaryDocumentTypeEdit"
        datasource="dicMilitaryDocumentTypeDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicMilitaryDocumentTypeDs"
                    class="kz.uco.tsadv.modules.personal.dictionary.DicMilitaryDocumentType"
                    view="dicMilitaryDocumentType-edit"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dicMilitaryDocumentTypeDs">
            <column width="250px">
                <field property="code"/>
                <field property="langValue1"/>
                <field property="langValue2"/>
                <field property="langValue3"/>
                <field property="langValue4"/>
                <field property="langValue5"/>
                <field property="startDate"/>
                <field property="endDate"/>
                <field property="company"/>
                </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
