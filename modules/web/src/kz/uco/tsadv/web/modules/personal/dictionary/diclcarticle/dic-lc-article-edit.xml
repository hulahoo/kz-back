<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicLCArticle.editorCaption"
        class="kz.uco.tsadv.web.modules.personal.dictionary.diclcarticle.DicLCArticleEdit"
        datasource="dicLCArticleDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicLCArticleDs"
                    class="kz.uco.tsadv.modules.personal.dictionary.DicLCArticle"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="scrollBox"
            spacing="true">
        <scrollBox id="scrollBox"
                   spacing="true">
            <fieldGroup id="fieldGroup"
                        datasource="dicLCArticleDs">
                <column width="250px">
                    <field property="code"/>
                    <field property="attribute"/>
                    <field property="article"/>
                    <field property="item"/>
                    <field property="subItem"/>
                    <field property="langValue1"/>
                    <field property="langValue2"/>
                    <field property="langValue3"/>
                    <field property="langValue4"/>
                    <field property="langValue5"/>
                    <field property="startDate"/>
                    <field property="endDate"/>
                </column>
            </fieldGroup>
        </scrollBox>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
