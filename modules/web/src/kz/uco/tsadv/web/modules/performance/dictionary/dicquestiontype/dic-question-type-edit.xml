<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicQuestionType.editorCaption"
        class="kz.uco.tsadv.web.modules.performance.dictionary.dicquestiontype.DicQuestionTypeEdit"
        datasource="dicQuestionTypeDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicQuestionTypeDs"
                    class="kz.uco.tsadv.modules.performance.dictionary.DicQuestionType"
                    view="dicQuestionType.browse"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dicQuestionTypeDs">
            <column width="250px">
                <field property="langValue1"/>
                <field property="langValue2"/>
                <field property="langValue3"/>
                <field property="langValue4"/>
                <field property="langValue5"/>
                <field property="code"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
