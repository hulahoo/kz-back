<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicDocumentType.editorCaption"
        class="kz.uco.tsadv.web.modules.personal.dictionary.dicdocumenttype.DicDocumentTypeEdit"
        datasource="dicDocumentTypeDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicDocumentTypeDs"
                    class="kz.uco.tsadv.modules.personal.dictionary.DicDocumentType"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dicDocumentTypeDs">
            <column width="250px">
                <field property="code"/>
                <field property="langValue1"/>
                <field property="langValue2"/>
                <field property="langValue3"/>
                <field property="langValue4"/>
                <field property="langValue5"/>
                <field property="startDate"/>
                <field property="endDate"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
