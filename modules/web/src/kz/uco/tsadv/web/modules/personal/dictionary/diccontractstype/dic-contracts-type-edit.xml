<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="kz.uco.tsadv.web.modules.personal.dictionary.diccontractstype.DicContractsTypeEdit"
        datasource="dicContractsTypeDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web.modules.personal.dictionary.diccontractstype">
    <dsContext>
        <datasource id="dicContractsTypeDs"
                    class="kz.uco.tsadv.modules.personal.dictionary.DicContractsType"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="scrollBox"
            spacing="true">
        <scrollBox id="scrollBox"
                   spacing="true">
            <fieldGroup id="fieldGroup"
                        datasource="dicContractsTypeDs">
                <column width="250px">
                    <field property="code"/>
                    <field property="langValue1"/>
                    <field property="langValue2"/>
                    <field property="langValue3"/>
                    <field property="langValue4"/>
                    <field property="langValue5"/>
                </column>
            </fieldGroup>
        </scrollBox>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
