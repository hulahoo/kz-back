<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="kz.uco.tsadv.web.modules.recognition.dictionary.dicrecognitiontype.DicRecognitionTypeEdit"
        datasource="dicRecognitionTypeDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web.modules.recognition.dictionary.dicrecognitiontype">
    <dsContext>
        <datasource id="dicRecognitionTypeDs"
                    class="kz.uco.tsadv.modules.recognition.dictionary.DicRecognitionType"
                    view="dicRecognitionType.edit"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="scrollBox"
            spacing="true">
        <scrollBox id="scrollBox"
                   spacing="true">
            <fieldGroup id="fieldGroup"
                        datasource="dicRecognitionTypeDs">
                <column width="250px">
                    <field property="langValue1"/>
                    <field property="description1"/>
                    <field property="langValue2"/>
                    <field property="description2"/>
                    <field property="langValue3"/>
                    <field property="description3"/>
                    <field property="langValue4"/>
                    <field property="description4"/>
                    <field property="langValue5"/>
                    <field property="description5"/>
                    <field property="startDate"/>
                    <field property="endDate"/>
                    <field property="code"/>
                    <field property="legacyId"/>
                    <field property="isSystemRecord"/>
                    <field property="coins"/>
                    <field property="allowCoinsEdit"/>
                    <field property="sticker"/>
                    <field property="emptySticker"/>
                    <field property="order"/>
                    <field property="company"/>
                </column>
            </fieldGroup>
        </scrollBox>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
