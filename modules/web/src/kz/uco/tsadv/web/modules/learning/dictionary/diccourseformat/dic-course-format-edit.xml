<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicCourseFormat.editorCaption"
        class="kz.uco.tsadv.web.modules.learning.dictionary.diccourseformat.DicCourseFormatEdit"
        datasource="dicCourseFormatDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicCourseFormatDs"
                    class="kz.uco.tsadv.modules.learning.dictionary.DicCourseFormat"
                    view="_local"/>
    </dsContext>
    <dialogMode forceDialog="true"
                height="AUTO"
                modal="true"
                width="600"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dicCourseFormatDs">
            <column width="250px">
                <field property="langValue1"/>
                <field property="langValue2"/>
                <field property="langValue3"/>
                <field property="langValue4"/>
                <field property="langValue5"/>
                <field property="startDate"/>
                <field property="endDate"/>
                <field property="code"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
