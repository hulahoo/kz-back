<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicJobCategory.editorCaption"
        class="kz.uco.tsadv.web.modules.personal.dictionary.dicjobcategory.DicJobCategoryEdit"
        datasource="dicJobCategoryDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicJobCategoryDs"
                    class="kz.uco.tsadv.modules.personal.dictionary.DicJobCategory"
                    view="dicJobCategory-edit"/>
    </dsContext>
    <dialogMode height="AUTO"
                modal="true"
                width="600"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dicJobCategoryDs">
            <column width="250px">
                <field property="langValue1"/>
                <field property="langValue2"/>
                <field property="langValue3"/>
                <field property="langValue4"/>
                <field property="langValue5"/>
                <field property="startDate"/>
                <field property="endDate"/>
                <field property="code"/>
                <field property="company"/>
                </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
