<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicRcQuestionnaireStatus.browseCaption"
        class="kz.uco.tsadv.web.modules.recruitment.dictionary.dicrcquestionnairestatus.DicRcQuestionnaireStatusBrowse"
        focusComponent="dicRcQuestionnaireStatusesTable"
        lookupComponent="dicRcQuestionnaireStatusesTable"
        messagesPack="kz.uco.tsadv.web.modules.recruitment.dictionary.dicrcquestionnairestatus">
    <dsContext>
        <groupDatasource id="dicRcQuestionnaireStatusesDs"
                         class="kz.uco.tsadv.modules.recruitment.dictionary.DicRcQuestionnaireStatus"
                         view="_local">
            <query>
                <![CDATA[select e from tsadv$DicRcQuestionnaireStatus e]]>
            </query>
        </groupDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="dicRcQuestionnaireStatusesTable"
            spacing="true">
        <filter id="filter"
                applyTo="dicRcQuestionnaireStatusesTable"
                datasource="dicRcQuestionnaireStatusesDs">
            <properties include=".*"/>
        </filter>
        <groupTable id="dicRcQuestionnaireStatusesTable"
                    width="100%">
            <actions>
                <action id="create"/>
                <action id="edit"/>
                <action id="remove"/>
            </actions>
            <columns>
                <column id="langValue1"/>
                <column id="langValue2"/>
                <column id="langValue3"/>
                <column id="langValue4"/>
                <column id="langValue5"/>
                <column id="startDate"/>
                <column id="endDate"/>
                <column id="code"/>
            </columns>
            <rows datasource="dicRcQuestionnaireStatusesDs"/>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn"
                        action="dicRcQuestionnaireStatusesTable.create"/>
                <button id="editBtn"
                        action="dicRcQuestionnaireStatusesTable.edit"/>
                <button id="removeBtn"
                        action="dicRcQuestionnaireStatusesTable.remove"/>
            </buttonsPanel>
        </groupTable>
    </layout>
</window>
