<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="kz.uco.tsadv.web.modules.recognition.dictionary.dicpersonpreferencetype.DicPersonPreferenceTypeBrowse"
        focusComponent="dicPersonPreferenceTypesTable"
        lookupComponent="dicPersonPreferenceTypesTable"
        messagesPack="kz.uco.tsadv.web.modules.recognition.dictionary.dicpersonpreferencetype">
    <dsContext>
        <groupDatasource id="dicPersonPreferenceTypesDs"
                         class="kz.uco.tsadv.modules.recognition.dictionary.DicPersonPreferenceType"
                         view="_local">
            <query>
                <![CDATA[select e from tsadv$DicPersonPreferenceType e]]>
            </query>
        </groupDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="dicPersonPreferenceTypesTable"
            spacing="true">
        <filter id="filter"
                applyTo="dicPersonPreferenceTypesTable"
                datasource="dicPersonPreferenceTypesDs">
            <properties include=".*"/>
        </filter>
        <groupTable id="dicPersonPreferenceTypesTable"
                    width="100%">
            <actions>
                <action id="create"/>
                <action id="edit"/>
                <action id="remove"/>
            </actions>
            <columns>
                <column id="code"/>
                <column id="coins"/>
                <column id="langValue1"/>
                <column id="langValue2"/>
                <column id="langValue3"/>
                <column id="langValue4"/>
                <column id="langValue5"/>
                <column id="startDate"/>
                <column id="endDate"/>
            </columns>
            <rows datasource="dicPersonPreferenceTypesDs"/>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn"
                        action="dicPersonPreferenceTypesTable.create"/>
                <button id="editBtn"
                        action="dicPersonPreferenceTypesTable.edit"/>
                <button id="removeBtn"
                        action="dicPersonPreferenceTypesTable.remove"/>
            </buttonsPanel>
        </groupTable>
    </layout>
</window>
