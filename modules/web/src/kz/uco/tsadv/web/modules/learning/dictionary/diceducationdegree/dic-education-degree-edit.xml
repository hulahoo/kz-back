<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicEducationDegree.editorCaption"
        class="kz.uco.tsadv.web.modules.learning.dictionary.diceducationdegree.DicEducationDegreeEdit"
        datasource="dicEducationDegreeDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicEducationDegreeDs"
                    class="kz.uco.tsadv.modules.learning.dictionary.DicEducationDegree"
                    view="dicEducationDegree-edit"/>
    </dsContext>
    <dialogMode forceDialog="true"
                height="AUTO"
                modal="true"
                width="600"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dicEducationDegreeDs">
            <column width="250px">
                <field property="langValue1"/>
                <field property="langValue2"/>
                <field property="langValue3"/>
                <field property="langValue4"/>
                <field property="langValue5"/>
                <field property="startDate"/>
                <field property="endDate"/>
                <field property="code"/>
                <field property="company"/>
                </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
