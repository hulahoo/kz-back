        class="kz.uco.tsadv.web.modules.personal.dictionary.dicattitudetomilitary.DicAttitudeToMilitaryEdit"
        datasource="dicAttitudeToMilitaryDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web">
