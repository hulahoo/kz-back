        <datasource id="dicAttitudeToMilitaryDs"
                    class="kz.uco.tsadv.modules.personal.dictionary.DicAttitudeToMilitary"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="scrollBox"
            spacing="true">
        <scrollBox id="scrollBox"
                   spacing="true">
            <fieldGroup id="fieldGroup"
                        datasource="dicAttitudeToMilitaryDs">
                <column width="250px">
                    <field property="code"/>
                    <field property="langValue1"/>
                    <field property="langValue2"/>
                    <field property="langValue3"/>
                    <field property="langValue4"/>
                    <field property="langValue5"/>
                    <field property="startDate"/>
                    <field property="endDate"/>
                </column>
            </fieldGroup>
        </scrollBox>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
