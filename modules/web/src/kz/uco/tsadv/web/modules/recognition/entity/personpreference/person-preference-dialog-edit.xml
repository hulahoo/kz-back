                width="600"/>
    <layout height="100%" expand="scrollBox" spacing="true" stylename="rcg-window-layout rcg-ch-preference-wrapper">
        <scrollBox id="scrollBox" width="100%" spacing="true" stylename="rcg-dialog-scroll b-scroll">

        </scrollBox>
        <hbox spacing="true" align="MIDDLE_RIGHT" stylename="rcg-window-footer" height="64px">
