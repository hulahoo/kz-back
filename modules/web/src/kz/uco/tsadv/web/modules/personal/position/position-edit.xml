<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://Position.editorCaption"
        class="kz.uco.tsadv.web.modules.personal.position.PositionEdit"
        datasource="positionDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="gradeGroupValueDs"
                    class="kz.uco.tsadv.modules.personal.model.GradeRuleValue"/>
        <datasource id="positionDs"
                    class="kz.uco.tsadv.modules.personal.model.PositionExt"
                    view="position-view">
            <datasource id="positionGroupDs"
                        property="group">
                <collectionDatasource id="assignmentsDs"
                                      property="assignments"/>
                <collectionDatasource id="surChargeDs"
                                      property="surCharge"/>
                <datasource id="analyticsDs" property="analytics"/>
            </datasource>
        </datasource>
        <collectionDatasource id="posCompetenceElementsDs"
                              class="kz.uco.tsadv.modules.personal.model.CompetenceElement"
                              view="competenceElement-view">
            <query>
                <![CDATA[
                        select e
                          from tsadv$CompetenceElement e
                         where e.positionGroup.id = :ds$positionGroupDs.id
                        ]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="casesDs"
                              class="kz.uco.tsadv.modules.personal.model.Case"
                              view="_minimal">
            <query>
                <![CDATA[
                        select e
                          from tsadv$Case e
                         where e.positionGroup.id = :ds$positionGroupDs.id
                        ]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="flySurChargeDs"
                              class="kz.uco.tsadv.modules.personal.model.FlySurCharge"
                              view="flySurCharge-view">
            <query>
                <![CDATA[
                        select e
                          from tsadv$FlySurCharge e
                         where e.positionGroupId.id = :ds$positionGroupDs.id
                        ]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="flightTimeRateDs"
                              class="kz.uco.tsadv.modules.personal.model.FlightTimeRate"
                              view="flightTimeRate-view">
            <query>
                <![CDATA[
                        select e
                          from tsadv$FlightTimeRate e
                         where e.positionGroupName.id = :ds$positionGroupDs.id
                        ]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="goalsDs"
                              class="kz.uco.tsadv.modules.personal.model.PositionGroupGoalLink"
                              view="positionGroupGoalLink.edit">
            <query>
                <![CDATA[
                        select e
                          from tsadv$PositionGroupGoalLink e
                         where e.positionGroup.id = :ds$positionGroupDs.id
                        ]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="hierarchyElementsDs"
                              class="kz.uco.tsadv.modules.personal.model.HierarchyElementExt"
                              view="hierarchyElement.parent">
            <query>
                <![CDATA[
                        select e
                          from base$HierarchyElementExt e
                         where e.positionGroup.id = :ds$positionGroupDs.id
                           and e.deleteTs is null
                      order by e.hierarchy, e.startDate
                        ]]>
            </query>
        </collectionDatasource>
        <groupDatasource id="parentElementsGoalsDs"
                         class="kz.uco.tsadv.modules.personal.model.ParentElementsGoal"
                         refreshMode="NEVER"/>
        <collectionDatasource id="organizationGroupExtDs"
                              class="kz.uco.tsadv.modules.personal.group.OrganizationGroupExt"
                              view="organizationGroup.browse">
            <query>
                <![CDATA[select e from base$OrganizationGroupExt e 
                join e.list l
                where e.id = l.group.id
                and :session$systemDate between
                l.startDate and l.endDate]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="vacationConditionsDs"
                              class="kz.uco.tsadv.modules.personal.model.VacationConditions"
                              view="vacationConditions-view">
            <query>
                <![CDATA[select a
                  from tsadv$VacationConditions a
                 where a.positionGroup.id = :ds$positionDs.group.id
                 order by a.startDate desc, a.endDate desc ]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="calendarsDs"
                              class="kz.uco.tsadv.modules.timesheet.model.Calendar"
                              view="calendar.view">
            <query>
                <![CDATA[select e from tsadv$Calendar e]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="offsetsDs"
                              class="kz.uco.tsadv.modules.timesheet.model.StandardOffset"
                              view="standardOffset.view">
            <query>
                <![CDATA[select e from tsadv$StandardOffset e]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="workingConditionsDs"
                              class="kz.uco.tsadv.modules.personal.dictionary.DicWorkingCondition"
                              view="_local">
            <query>
                <![CDATA[select e from tsadv$DicWorkingCondition e]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="actualAssignmentsDs"
                              class="kz.uco.tsadv.modules.personal.model.AssignmentExt"
                              view="assignmanetExt.view.for.positionEdit">
            <query>
                <![CDATA[select e from base$AssignmentExt e
                         where e.positionGroup.id = :ds$positionGroupDs.id
                         and e.startDate <= :session$systemDate
                         and e.assignmentStatus.code <> 'TERMINATED']]>
            </query>
        </collectionDatasource>
        <groupDatasource id="allowableSchedulesDs"
                         class="kz.uco.tsadv.modules.timesheet.model.AllowableScheduleForPosition"
                         view="allowableScheduleForPosition-view">
            <query>
                <![CDATA[select e from tsadv$AllowableScheduleForPosition e
                where e.positionGroup.id = :ds$positionGroupDs.id
                and :session$systemDate between e.startDate and e.endDate]]>
            </query>
        </groupDatasource>
        <collectionDatasource id="dicHrRolesDs"
                              class="kz.uco.tsadv.modules.personal.dictionary.DicHrRole"
                              view="_local">
            <query>
                <![CDATA[select e from tsadv$DicHrRole e]]>
            </query>
        </collectionDatasource>
        <collectionDatasource id="positionHarmfulConditionDs"
                class="kz.uco.tsadv.entity.tb.PositionHarmfulCondition"
                view="_local">
            <query>
                <![CDATA[select e from tsadv_PositionHarmfulCondition e
                where e.positionGroup.id = :ds$positionGroupDs.id]]>
            </query>

        </collectionDatasource>
    </dsContext>
