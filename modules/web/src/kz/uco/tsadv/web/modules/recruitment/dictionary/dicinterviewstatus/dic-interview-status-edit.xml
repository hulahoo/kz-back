<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="mainMsg://DicInterviewStatus.editorCaption"
        class="kz.uco.tsadv.web.modules.recruitment.dictionary.dicinterviewstatus.DicInterviewStatusEdit"
        datasource="dicInterviewStatusDs"
        focusComponent="fieldGroup">
    <dsContext>
        <datasource id="dicInterviewStatusDs"
                    class="kz.uco.tsadv.modules.recruitment.dictionary.DicInterviewStatus"
                    view="dicInterviewStatus-edit"/>
    </dsContext>
    <dialogMode height="AUTO"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dicInterviewStatusDs">
            <column width="250px">
                <field property="langValue1"/>
                <field property="langValue2"/>
                <field property="langValue3"/>
                <field property="langValue4"/>
                <field property="langValue5"/>
                <field property="startDate"/>
                <field property="endDate"/>
                <field property="code"/>
                <field property="company"/>
                </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
