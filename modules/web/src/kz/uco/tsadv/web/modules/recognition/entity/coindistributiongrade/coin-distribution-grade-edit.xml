<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="kz.uco.tsadv.web.modules.recognition.entity.coindistributiongrade.CoinDistributionGradeEdit"
        datasource="coinDistributionGradeDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web.modules.recognition.entity.coindistributiongrade">
    <dsContext>
        <datasource id="coinDistributionGradeDs"
                    class="kz.uco.tsadv.modules.recognition.CoinDistributionGrade"
                    view="coinDistributionGrade.edit"/>
    </dsContext>
    <dialogMode forceDialog="true"
                height="AUTO"
                modal="true"
                width="AUTO"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="coinDistributionGradeDs">
            <column width="250px">
                <field property="gradeGroup" captionProperty="grade"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
