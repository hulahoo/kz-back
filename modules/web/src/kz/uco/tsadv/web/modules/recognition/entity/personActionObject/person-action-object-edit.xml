<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="kz.uco.tsadv.web.modules.recognition.entity.personActionObject.PersonActionObjectEdit"
        datasource="personActionObjectDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web.modules.recognition.entity.personActionObject">
    <dsContext>
        <datasource id="personActionObjectDs"
                    class="kz.uco.tsadv.modules.recognition.PersonActionObject"
                    view="personActionObject-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="personActionObjectDs">
            <column width="250px">
                <field property="object"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
