<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="kz.uco.tsadv.web.talentprogramstepskill.TalentProgramStepSkillEdit"
        datasource="talentProgramStepSkillDs"
        focusComponent="fieldGroup"
        messagesPack="kz.uco.tsadv.web.talentprogramstepskill">
    <dsContext>
        <datasource id="talentProgramStepSkillDs"
                    class="kz.uco.tsadv.entity.TalentProgramStepSkill"
                    view="talentProgramStepSkill-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="talentProgramStepSkillDs">
            <column width="250px">
                <field caption="msg://orderNumber"
                       property="orderNumber"/>
                <field caption="msg://skill"
                       property="skill"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
